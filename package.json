{
  "name": "ewelink-api-next",
  "version": "1.0.1",
  "description": "CoolKit v2 API Official Library",
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && tsc && rm -rf dist/test && cp -r dist/src/* dist && rm -rf dist/src",
    "build:all": "rimraf dist && tsc",
    "test": "cross-env TS_NODE_PROJECT='test/tsconfig.test.json' mocha test/*.test.ts --timeout 30000",
    "testJson": "cross-env TS_NODE_PROJECT='test/tsconfig.test.json' mocha test/*.test.ts --reporter json --reporter-option output=test.json",
    "cover": "nyc --reporter=html npm run test",
    "build:rollup": "rimraf dist && rollup -c"
  },
  "keywords": [
    "coolkit",
    "ewelink"
  ],
  "type": "module",
  "author": "nocmt",
  "license": "MIT",
  "devDependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/ws": "^8.5.3",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-typescript": "^8.3.4",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.6.2",
    "@types/node-localstorage": "^1.3.0",
    "@types/sinon": "^10.0.12",
    "chai": "^4.3.6",
    "cross-env": "^7.0.3",
    "mocha": "10.1.0",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.77.3",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.32.1",
    "sinon": "^14.0.0",
    "ts-node": "^10.8.2",
    "tsconfig-paths": "^4.0.0",
    "tslib": "^2.4.0",
    "typescript": "4.8.0-dev.20220728"
  },
  "dependencies": {
    "axios": "1.2.1",
    "bonjour-service": "1.0.14",
    "crypto-js": "^4.1.1",
    "dayjs": "1.11.7",
    "log4js": "6.7.1",
    "node-localstorage": "^2.2.1",
    "ws": "8.11.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coolkit-carl/ewelink-api-next.git"
  },
  "bugs": {
    "url": "https://github.com/coolkit-carl/ewelink-api-next/issues"
  },
  "files": [
    "dist",
    "README.zh-CN.md",
    "package.json",
    "CHANGELOG.md",
    "docs"
  ]
}